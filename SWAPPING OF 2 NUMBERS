SWAP TWO NUMBERS 

import java.util.Scanner;
class pjee2
{
 public static void main(String[] args)
 {
  Scanner sc = new Scanner (System.in);
  System.out.println("enter 1 the number ");
  int a = sc.nextInt();
  System.out.println("enter 2 the number ");
  int b = sc.nextInt();
  int c = a;
  a=b;
  b=c;
  sc.close();
  System.out.println("the sum is "+a+" "+b);
  
 }
}
